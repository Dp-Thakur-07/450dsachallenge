
 int totalCount(int k, vector<int>& arr) {
        // code here
        int total = 0;
        for(int i=0;i<arr.size();i++){
            if(arr[i]%k==0){
                total+=(arr[i]/k);
            }
            else{
                total+=(arr[i]/k+1);
            }
        }
        return total;
    }
